mkdir Dev
xcopy /Y "$(TargetDir)DevExpress*.dll" "$(TargetDir)Dev\"

echo "拷贝到Framework文件夹下"
xcopy /Y "$(TargetDir)JCodes.Framework.BLL.dll" "$(TargetDir)Framework\"
